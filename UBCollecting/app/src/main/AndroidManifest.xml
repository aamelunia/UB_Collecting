<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.buffalo.cse.ubcollecting">
    <uses-permission android:name="android.permission.CAMERA" android:required="true"/>
    <uses-feature
        android:name="android.hardware.camera.any"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PersonActivity" />
        <activity android:name=".FieldTripActivity" />
        <activity android:name=".FileActivity" />
        <activity android:name=".LanguageActivity" />
        <activity android:name=".QuestionnaireActivity" />
        <activity android:name=".QuestionnaireContentActivity" />
        <activity android:name=".QuestionnaireTypeActivity" />
        <activity android:name=".RoleActivity" />
        <activity android:name=".SessionActivity" />
        <activity android:name=".AnswerActivity" />
        <activity android:name=".LanguageTypeActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".QuestionLangVersionActivity" />
        <activity android:name=".QuestionOptionActivity" />
        <activity android:name=".QuestionPropertyActivity" />
        <activity android:name=".QuestionPropertyDefActivity" />
        <activity android:name=".SessionAnswerActivity" />
        <activity android:name=".SessionPersonActivity" />
        <activity android:name=".TableListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TableViewActivity"></activity>
    </application>

</manifest>